{
  "config": {
    "name": "paperplane",
    "label": "Paperplane.io",
    "description": "Radically simple static hosting."
  },
  "records": [
    {
      "name": "",
      "type": "URL",
      "content": "http://www.{{domain}}",
      "ttl": 3600
    },
    {
      "name": "www",
      "type": "CNAME",
      "content": "proxy.paperplane.io",
      "ttl": 3600
    }
  ]
}
